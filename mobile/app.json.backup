{
  "expo": {
    "name": "Medical Platform",
    "slug": "medical-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medicalplatform.app"
    },
    "android": {
      "package": "com.medicalplatform.app"
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "medical-platform"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "medicalplatform"
  }
}
